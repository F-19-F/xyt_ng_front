				"enablePullDownRefresh": true
			}
		}, {
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "发现",
				"enablePullDownRefresh": false
			}
		}, {
			"path": "pages/me/me",
			"style": {
				"navigationBarTitleText": "关于",
				"enablePullDownRefresh": false
			}
		}, {
			"path": "pages/index/grade/grade",
			"style": {
				"navigationBarTitleText": "成绩查询",
				"enablePullDownRefresh": false
			}
		}, {
			"path": "pages/index/exam/exam",
			"style": {
				"navigationBarTitleText": "考试查询",
				"enablePullDownRefresh": false
			}

		}, {
			"path": "pages/index/classroom/classroom",
			"style": {
				"navigationBarTitleText": "空教室查询",
